

{{ form_start(form) }}
    <div class="col-md-12">
        {{ form_row(form.name) }}
    </div>
    <div class="col-md-12 mt-3">
        {{ form_row(form.surname) }}
    </div>
    <div class="col-md-12 mt-3">
        {{ form_row(form.license) }}
    </div>
    <div class="d-flex align-items-center justify-content-end mt-4">
        <button id="submit" class="btn btn-primary mt-4">{{ button_label|default('Save') }}</button>
    </div>
{{ form_end(form) }}