{{ form_start(form) }}
    <div class="col-md-12">
        {{ form_row(form.brand) }}
    </div>
    <div class="col-md-12 mt-3">
        {{ form_row(form.model) }}
    </div>
    <div class="col-md-12 mt-3">
        {{ form_row(form.plate) }}
    </div>
    <div class="col-md-12 mt-3">
        {{ form_row(form.license_required) }}
    </div>
    <div class="d-flex align-items-center justify-content-end mt-4">
        <button id="submit" class="btn btn-primary">{{ button_label|default('Save') }}</button>
    </div>
{{ form_end(form) }}
                